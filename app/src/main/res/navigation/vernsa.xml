<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/vernsa"
    app:startDestination="@id/splashScreen">

    <fragment
        android:id="@+id/splashScreen"
        android:name="com.example.vernsa.SplashScreen"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreen_to_viewpager2"
            app:destination="@id/viewpager2"
            app:popUpTo="@id/splashScreen"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashScreen_to_landingPage"
            app:destination="@id/landingPage"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/splashScreen"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginPage"
        android:name="com.example.vernsa.LoginPage"
        android:label="fragment_login_page"
        tools:layout="@layout/fragment_login_page" >
        <action
            android:id="@+id/action_loginPage_to_landingPage"
            app:destination="@id/landingPage"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_loginPage_to_forgotPassword"
            app:destination="@id/forgotPassword" />
        <action
            android:id="@+id/action_loginPage_to_personalAccount"
            app:destination="@id/personalAccount" />
    </fragment>
    <fragment
        android:id="@+id/personalAccount"
        android:name="com.example.vernsa.PersonalAccount"
        android:label="fragment_personal_account"
        tools:layout="@layout/fragment_personal_account" >
        <action
            android:id="@+id/action_personalAccount_to_landingPage"
            app:destination="@id/landingPage"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/landingPage"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_personalAccount_to_loginPage"
            app:destination="@id/loginPage" />
    </fragment>
    <fragment
        android:id="@+id/landingPage"
        android:name="com.example.vernsa.LandingPage"
        android:label="fragment_landing_page"
        tools:layout="@layout/fragment_landing_page" >
        <action
            android:id="@+id/action_landingPage_to_loginPage"
            app:destination="@id/loginPage"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_landingPage_to_personalAccount"
            app:destination="@id/personalAccount"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_in_left" />
    </fragment>
    <fragment
        android:id="@+id/viewpager2"
        android:name="com.example.vernsa.onboarding.viewpager"
        android:label="fragment_viewpager"
        tools:layout="@layout/fragment_viewpager" >
        <action
            android:id="@+id/action_viewpager2_to_landingPage"
            app:destination="@id/landingPage"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/viewpager2"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/forgotPassword"
        android:name="com.example.vernsa.ForgotPassword"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPassword_to_verifyPassword"
            app:destination="@id/verifyPassword" />
    </fragment>
    <fragment
        android:id="@+id/verifyPassword"
        android:name="com.example.vernsa.VerifyPassword"
        android:label="fragment_verify_password"
        tools:layout="@layout/fragment_verify_password" >
        <action
            android:id="@+id/action_verifyPassword_to_verified"
            app:destination="@id/verified" />
    </fragment>
    <Dia
        android:id="@+id/verified"
        android:name="com.example.vernsa.Verified"
        android:label="fragment_verified"
        tools:layout="@layout/fragment_verified" />
</navigation>